;===============================================================================
; Definition of the Basic Action
;===============================================================================

;-------------------------------------------------------------------------------
; ダッシュ
[Statedef 100]
type    = S
movetype = I
physics = N
anim = 100
ctrl = 0

[State 100,0]
type = VelSet
trigger1 = AnimElem = 4
x = Const(velocity.run.fwd.x)

[State 100,1]
type = PlaySnd
trigger1 = TimeMod = 70, 0
value = 100, 0

[State 100,2]
type = PlaySnd
trigger1 = TimeMod = 70, 21
value = 100, 1

[State 100,3]
type = PlaySnd
trigger1 = TimeMod = 70, 47
value = 100, 2

[State 100,2040]
type = ChangeState
trigger1 = Var(30)!=0 && Random <300
trigger1 = P2BodyDist X = [130,180]
trigger1 = P2StateType = S || P2StateType = C
value = 2040
ctrl = 0

[State 100,4050]
type = ChangeState
triggerall = Var(30)!=0 && Random < 200
triggerall = P2BodyDist X = [10,90]
triggerall = P2StateType = S || P2StateType = C
trigger1 = P2BodyDist X = [50,90]
trigger1 = P2MoveType = A && P2Stateno = [200,499]
trigger2 = P2MoveType = A && P2Stateno >= 1000
trigger2 = Random < 800 && P2BodyDist X = [10,70]
value = IfElse(Random<800,4050,4150)
ctrl = 0

[State 100,101]
type = ChangeState
trigger1 = Command != "holdfwd" && Time >= 20
trigger2 = Var(30)!=0 && Time >= 20
trigger2 = enemy,NumProj >=1 && Random <100
trigger3 = Var(30)!=0 && Time >= 20
trigger3 = P2BodyDist X <= 50 && Random <100
trigger4 = Var(30)!=0 && Time >= 20
trigger4 = P2MoveType = A
trigger4 = P2BodyDist X <= 100 && Random <100
trigger5 = Var(30)!=0 && Time >= 20
trigger5 = P2MoveType = A
trigger5 = Random < 10
value = 101
ctrl = 1

[State 100,ChangeState]
type = ChangeState
trigger1 = Command = "holdupfwd" && Time >= 15
value = 40

[State 100,4]
type = AssertSpecial
trigger1 = 1
flag = NoWalk

;------------------------ 停止
[Statedef 101]
type = S
movetype = I
physics = S
anim = 101
velset = 0, 0

[State 101,End]
type = ChangeState
trigger1 = AnimTime = 0
value = 0
ctrl = 1

