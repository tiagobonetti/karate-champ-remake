ctrl = 1

[State 100,ChangeState]
type = ChangeState
triggerall = !(var(30)!=0 && var(31)!=0 && var(32)!=0)
trigger1 = Command = "holdupfwd" && Time >= 4
value = 40

[State 100,4]
type = AssertSpecial
trigger1 = 1
flag = NoWalk

