trigger1 = !(var(30)!=0 && var(31)!=0 && var(32)!=0)
trigger2 = Var(30)!=0 && Time >= 15
trigger2 = enemy,NumProj >=1 && Random <100
trigger3 = Var(30)!=0 && Time >= 15
trigger3 = P2BodyDist X <= 50 && Random <100
