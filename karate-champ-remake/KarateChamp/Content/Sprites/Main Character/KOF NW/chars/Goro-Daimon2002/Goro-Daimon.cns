;===============================
; Goro Daimon X'S BASIC CODING
;===============================

[Data]
life = 1500          ;Amount of life to start with
attack = 100         ;100% attack power (more is stronger)
defence = 100        ;100% defensive power (more is stronger)
fall.defence_up = 90 ;Def goes up by 50% everytime player is hit on ground
liedown.time = 60    ;Time player takes before getting up from HIT_LIEDOWN
airjuggle = 15       ;Can do 15 points of juggling
sparkno = 0          ;Default spark number
guard.sparkno = 0    ;Default guard spark number
volume = 30
IntPersistIndex=1
FloatPersistIndex=0
KO.echo = 0          ;1 to enable echo on KO
power = 5000

[Size]
xscale = 1           ;Horizontal scaling factor.
yscale = 1           ;Vertical scaling factor.
ground.back = 16     ;Player width (back, ground)
ground.front = 15    ;Player width (front, ground)
air.back = 12        ;Player width (back, air)
air.front = 11       ;Player width (front, air)
z.width = 3          ;Player width (into and out)
attack.width = 0     ;Default attack width: back, front
attack.dist = 160    ;Default attack distance
proj.attack.dist = 90;Default attack distance for projectiles
head.pos = -7, -114  ;Approximate position of head
mid.pos = -2, -74    ;Approximate position of midsection

[Velocity]
walk.fwd  = 2.301952       ;Walk forward
walk.back = -2.301952      ;Walk backward
run.fwd  = 4.62            ;Run forward (x, y)  - if y<0, player will hop
run.back = -4.3,-5         ;Run backward (x, y) - if y=0, player will run
jump.neu = 0,-7.75         ;Neutral jumping velocity (x, y)
jump.back = -2.2,-7.75     ;Jump back Speed (x, y)
jump.fwd = 2.2,-7.75       ;Jump forward Speed (x, y)
runjump.fwd = 2.6,-7.75    ;.
airjump.neu = 0,-7.75      ;.
airjump.back = -2.55,-7.75 ;Air jump speeds (opt)
airjump.fwd = 2.5,-7.75    ;.

[Movement]
airjump.num = 0            ;Number of air jumps allowed (opt)
airjump.height = 35        ;Minimum distance from ground before you can air jump (opt)
yaccel = .435              ;Vertical acceleration
stand.friction = .800      ;Friction coefficient when standing
crouch.friction = .795     ;Friction coefficient when crouching

