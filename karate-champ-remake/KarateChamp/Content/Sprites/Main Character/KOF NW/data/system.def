enabled = 1
winstext.text = Rounds survived: %i  ;Use %i for win counter
winstext.font = 3,0,0
winstext.offset = 159,70
winstext.displaytime = -1
winstext.layerno = 2
fadein.time = 32
show.time = 300
fadeout.time = 64
roundstowin = 5           ;Number of rounds to get win pose (lose pose otherwise)

